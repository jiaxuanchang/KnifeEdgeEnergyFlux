   "source": [
    "H=200\n",
    "alpha=0.3\n",
    "Lx=1/alpha*H\n",
    "dx=np.floor(Lx/40)\n",
    "dz=H/50\n",
    "z=np.array([np.arange(0,-H,-dz)]).T\n",
    "x1=np.arange(0,1.5*Lx,dx)[np.newaxis]\n",
    "x2=np.arange(-1.5*Lx,1,dx)[np.newaxis]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "u1=np.zeros((len(z),np.shape(x1)[-1],nmodes))\n",
    "u2=np.zeros((len(z),np.shape(x2)[-1],nmodes))\n",
    "\n",
    "for i in range(nmodes):\n",
    "    u1[:,:,i]=an[i]*np.cos((i+1)*np.pi*(z)/H)*np.cos(alpha*(i+1)*np.pi/H*x1)\n",
    "    u2[:,:,i]=an[i]*np.cos((i+1)*np.pi*(z)/H)*np.cos(-alpha*(i+1)*np.pi/H*x2)"
   ]
